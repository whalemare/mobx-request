    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "standard-version": "9.0.0",
    "ts-node": "9.0.0",
    "typescript": "4.5.4",
    "vscode-generate-index-standalone": "1.6.0"
